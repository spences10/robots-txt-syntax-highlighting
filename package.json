{
  "name": "robots.txt",
  "displayName": "robots.txt syntax highlighting",
  "description": "Syntax highlighting for robots.txt files",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.83.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [{
      "id": "robots-txt",
      "aliases": ["robots.txt", "robots-txt"],
      "extensions": [".txt"],
      "configuration": "./language-configuration.json"
    }],
    "grammars": [{
      "language": "robots-txt",
      "scopeName": "source.robots-txt",
      "path": "./syntaxes/robots-txt.tmLanguage.json"
    }]
  }
}
