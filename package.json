{
  "name": "robots-txt",
  "displayName": "robots.txt",
  "description": "Simple syntax highlighting for robots.txt files",
  "version": "0.0.5",
  "publisher": "spences10",
  "icon": ".github/assets/openmoji-robot.png",
  "repository": {
    "type": "git",
    "url": "https://github.com/spences10/robots-txt-syntax-highlighting.git"
  },
  "engines": {
    "vscode": "^1.83.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [
      {
        "id": "robots-txt",
        "aliases": [
          "robots.txt",
          "robots-txt"
        ],
        "extensions": [
          ".txt"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "robots-txt",
        "scopeName": "source.robots-txt",
        "path": "./syntaxes/robots-txt.tmLanguage.json"
      }
    ]
  }
}
